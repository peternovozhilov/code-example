{
    "name": "champinpoker/imsparsers",
    "description": "IMS reports parsing example",
    "type": "project",
    "require": {
		"php-webdriver/webdriver": "^1.14",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\YourProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\YourProject\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform-check": true,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
